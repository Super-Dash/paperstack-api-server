{
  "name": "paperstack-api-server",
  "version": "0.1.1",
  "description": "作业收集平台PaperStack的API服务器",
  "main": "bin/app.js",
  "repository": "https://github.com/super-dash/paperstack-api-server.git",
  "author": "github.com/super-dash",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf bin/*",
    "lint": "esw src --ext .ts",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint:fix --watch",
    "build": "babel src --extensions .ts --out-dir bin",
    "build:watch": "yarn build --watch",
    "watch": "npm-run-all --parallel lint:watch build:watch",
    "start": "node bin/app.js",
    "start:dev": "nodemon bin/app.js --watch bin",
    "test": "echo No test sepcified."
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.4.0",
    "eslint-watch": "^6.0.0",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/register": "^7.6.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.27",
    "@types/express": "^4.17.1",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^12.7.5",
    "@types/randomstring": "^1.1.6",
    "@types/sequelize": "^4.28.4",
    "@types/validator": "^10.11.3",
    "bcrypt": "^3.0.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "js-yaml": "^3.13.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "randomstring": "^1.1.5",
    "sequelize": "^5.19.0",
    "typescript": "^3.6.3"
  }
}
