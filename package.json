{
  "name": "paperstack-api-server",
  "version": "0.1.2",
  "description": "作业收集平台PaperStack的API服务器",
  "main": "lib/app.js",
  "repository": "https://github.com/super-dash/paperstack-api-server.git",
  "author": "github.com/super-dash",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./lib && rm -rf ./log",
    "lint": "eslint src --ext .ts",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "watch \"yarn lint:fix\" src --wait 10",
    "build": "babel src --extensions .ts --out-dir lib",
    "build:watch": "yarn build --watch",
    "watch": "watch \"npm-run-all lint:fix build\" src --wait 10",
    "start": "node lib/app.js",
    "start:dev": "nodemon lib/app.js --watch lib --delay 10",
    "test": "echo \"No test sepcified.\""
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.27",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^12.7.5",
    "@types/randomstring": "^1.1.6",
    "@types/sequelize": "^4.28.4",
    "@types/validator": "^10.11.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "eslint": "^6.4.0",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/register": "^7.6.0",
    "bcrypt": "^3.0.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "js-yaml": "^3.13.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "randomstring": "^1.1.5",
    "sequelize": "^5.19.0",
    "typescript": "^3.6.3"
  }
}
